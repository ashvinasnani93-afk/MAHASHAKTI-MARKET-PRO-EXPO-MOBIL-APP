{
  "expo": {
    "name": "Mahashakti Market Pro",
    "slug": "mahashakti-market-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mahashakti",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "backgroundColor": "#0a0a0a",
    "primaryColor": "#3b82f6",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mahashakti.marketpro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.mahashakti.marketpro",
      "permissions": [],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0a0a0a"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "mahashakti-market-pro"
      }
    }
  }
}
